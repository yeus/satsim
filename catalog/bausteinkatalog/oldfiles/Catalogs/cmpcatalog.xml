<?xml version="1.0" encoding="ISO-8859-1"?>
<Catalog
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:xlink="http://www.w3.org/1999/xlink"
    xmlns:VSD="http://www.VEROSIM.de/namespaces/VSD"
    xmlns="http://www.VEROSIM.de/namespaces/IBOSS"
    xsi:schemaLocation="http://www.VEROSIM.de/namespaces/IBOSS ../XSD/iboss.xsd">

    <componentDefs>
        
        <ThrusterDef VSD:id="id1">
            <VSD:name>Kaltgasantriebssystem</VSD:name>
            <size unit="m">0.2 0.2 0.2</size>
            <mass unit="g">2500</mass>
            <maxPowerConsumption unit="W">55</maxPowerConsumption>
            <standbyPowerConsumption unit="W">5</standbyPowerConsumption>
            <switchable>true</switchable>
        </ThrusterDef>
        
        <BatteryDef VSD:id="id2">
            <mass unit="kg">2</mass>
            <power unit="W">1500</power>
            <capacity unit="Ah">150</capacity>
        </BatteryDef>
        
        <AntennaDef VSD:id="id3">
            <size unit="m">0.2 0.2 0.2</size>
            <mass unit="kg">10</mass>
        </AntennaDef>
        
        <AntennaDef VSD:id="id4">
            <size unit="m">0.2 0.2 0.2</size>
            <mass unit="kg">2</mass>
        </AntennaDef>
        
        <MomentumWheelDef VSD:id="id5">
            <size unit="m">0.2 0.2 0.2</size>
            <mass unit="kg">2</mass>
        </MomentumWheelDef>
        
        <StorageTankDef VSD:id="id6">
            <volume unit="m³">0.15</volume>
        </StorageTankDef>
       
        <StructureDef VSD:id="id7">
            <VSD:name>1x1x1</VSD:name>
            <size unit="m">0.4 0.4 0.4</size>
            <mass unit="kg">5</mass>
            <heatCapacity unit="J/K">10</heatCapacity>
            <centerOfMass unit="m">0.2 0.2 0.2</centerOfMass>
            <inertiaTensor unit="kg·m²">
                1 0 0 
                0 1 0
                0 0 1
            </inertiaTensor>
        </StructureDef>

        <StructureDef VSD:id="id8">
            <VSD:name>2x2x2</VSD:name>
            <size unit="m">0.81 0.81 0.81</size>
            <mass unit="kg">30</mass>
            <heatCapacity unit="J/K">60</heatCapacity>
            <centerOfMass unit="m">0.41 0.41 0.41</centerOfMass>
            <inertiaTensor unit="kg·m²">
                1 0 0 
                0 1 0
                0 0 1
            </inertiaTensor>
        </StructureDef>
        
    </componentDefs>

    <buildingBlockDefs/>
</Catalog>

